{
  "entrypoint": "src\\index.html",
  "shell": "src\\index.js",
  "fragments": [],
  "sources": [
    "src/**/*",
    "src/index.html",
    "src/index.js"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js"
  ],
  "builds": [
    {
      "name": "Lit",
      "js": {
        "minify": false,
        "compile": "es5",
        "transformModulesToAmd": false
      },
      "css": {
        "minify": false
      },
      "html": {
        "minify": false
      },
      "bundle": true,
      "addServiceWorker": true,
      "addPushManifest": false,
      "preset": "es5-bundled"
    }
  ],
  "moduleResolution": "node"
}